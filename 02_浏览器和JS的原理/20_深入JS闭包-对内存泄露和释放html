<body>
  <script>
    // function  adder(num, count) {
    //   return num + 5
    // }
    // add(100,5)
    // add(55,5)
    // add(12,5)
    // add(22,8)
    // add(35,8)
    // add(7,8)

    function createAdder(count) {
      function adder(num) {
        return count + num
      }
      return adder
    }
    var adder5 = createAdder(5)
    adder5(100)
    adder5(55)
    var adder8 = createAdder(8)
    adder8(22)
    adder8(32)

    //永远不会使用adder8
    //内存泄漏:对于那些我们永远不会使用的对象,但是对于GC来说,他不知道要进行释放的对应内存仍然会保留
    adder8 = null
  </script>
</body>
